{
	"presets": [
    "@babel/preset-react",
		[
      "@babel/env",
      {
        "corejs": {
          "version": 3,
          "proposals": true // 使用尚在“提议”阶段特性的 polyfill
        }
      }
    ],
    "@babel/typescript"
	],
	"plugins": [
		"@babel/plugin-syntax-dynamic-import",
		"transform-class-properties"
	]
}
